{
  "name": "GetPathState",
  "arguments": [
    {
      "name": "id",
      "desc": "Path planner id. Default value is 0.",
      "optional": true,
      "type": "number"
    }
  ],
  "returns": [
    {
      "name": "state",
      "desc": "Current path planning state",
      "optional": false,
      "type": "string"
    }
  ],
  "examples": [
    [
      "function init()",
      "\tQueryPath(Vec(-10, 0, 0), Vec(10, 0, 0))",
      "end",
      "",
      "function tick()",
      "\tlocal s = GetPathState()",
      "\tif s == \"done\" then",
      "\t\tDebugPrint(\"done\")",
      "\tend",
      "end"
    ]
  ],
  "description": [
    "Return the current state of the last path planning query.",
    "${table:PathState}"
  ]
}
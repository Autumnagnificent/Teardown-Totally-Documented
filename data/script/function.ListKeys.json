{
  "name": "ListKeys",
  "arguments": [
    {
      "name": "parent",
      "desc": "The parent registry key",
      "optional": false,
      "type": "string"
    }
  ],
  "returns": [
    {
      "name": "children",
      "desc": "Indexed table of strings with child keys",
      "optional": false,
      "type": "table"
    }
  ],
  "examples": [
    [
      "--If the registry looks like this:",
      "--\tgame",
      "--\t\ttool",
      "--\t\t\tsteroid",
      "--\t\t\trifle",
      "--\t\t\t...",
      "",
      "function init()",
      "\tlocal list = ListKeys(\"game.tool\")",
      "\tfor i=1, #list do",
      "\t\tDebugPrint(list[i])",
      "\tend",
      "end",
      "",
      "--This will output:",
      "--steroid",
      "--rifle",
      "-- ..."
    ]
  ],
  "description": [
    "List all child keys of a registry node."
  ]
}
{
  "name": "GetScreenShape",
  "arguments": [
    {
      "name": "screen",
      "desc": "Screen handle",
      "optional": false,
      "type": "number"
    }
  ],
  "returns": [
    {
      "name": "shape",
      "desc": "Shape handle or zero if none",
      "optional": false,
      "type": "number"
    }
  ],
  "examples": [
    [
      "local screen = 0",
      "function init()",
      "\tscreen = FindScreen(\"tv\")",
      "\tlocal shape = GetScreenShape(screen)",
      "\tDebugPrint(shape)",
      "end"
    ]
  ],
  "description": [
    "Return handle to the parent shape of a screen"
  ]
}
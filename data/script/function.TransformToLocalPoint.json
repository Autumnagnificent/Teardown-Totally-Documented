{
  "name": "TransformToLocalPoint",
  "arguments": [
    {
      "name": "t",
      "desc": "Transform",
      "optional": false,
      "type": "TTransform"
    },
    {
      "name": "p",
      "desc": "Vector representing position",
      "optional": false,
      "type": "TVec"
    }
  ],
  "returns": [
    {
      "name": "r",
      "desc": "Transformed position",
      "optional": false,
      "type": "TVec"
    }
  ],
  "examples": [
    [
      "function init()",
      "\tlocal t = GetBodyTransform(body)",
      "\tlocal worldOrigo = Vec(0, 0, 0)",
      "\tlocal p = TransformToLocalPoint(t, worldOrigo)",
      "",
      "\t--p now represents the position of world origo in local body space",
      "\tDebugPrint(VecStr(p))",
      "end"
    ]
  ],
  "description": [
    "Transfom position p into transform t."
  ]
}
{
  "name": "GetPostProcessingProperty",
  "arguments": [
    {
      "name": "name",
      "desc": "Property name",
      "optional": false,
      "type": "string"
    }
  ],
  "returns": [
    {
      "name": "value0",
      "desc": "Property value",
      "optional": false,
      "type": "number"
    },
    {
      "name": "value1",
      "desc": "Property value (only some properties)",
      "optional": false,
      "type": "number"
    },
    {
      "name": "value2",
      "desc": "Property value (only some properties)",
      "optional": false,
      "type": "number"
    }
  ],
  "examples": [
    [
      "function tick()",
      "\tSetPostProcessingProperty(\"saturation\", 0.4)",
      "\tSetPostProcessingProperty(\"colorbalance\", 1.3, 1.0, 0.7)",
      "\tlocal saturation = GetPostProcessingProperty(\"saturation\")",
      "\tlocal r,g,b = GetPostProcessingProperty(\"colorbalance\")",
      "\tDebugPrint(\"saturation \" .. saturation)",
      "\tDebugPrint(\"colorbalance \" .. r .. \" \" .. g .. \" \" .. b)",
      "end"
    ]
  ],
  "description": [
    "This function is used for querying the current post processing properties. ",
    "The available properties are exactly the same as in the editor."
  ]
}
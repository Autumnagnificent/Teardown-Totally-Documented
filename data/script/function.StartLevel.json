{
  "name": "StartLevel",
  "arguments": [
    {
      "name": "mission",
      "desc": "An identifier of your choice",
      "optional": false,
      "type": "string"
    },
    {
      "name": "path",
      "desc": "Path to level XML file",
      "optional": false,
      "type": "string"
    },
    {
      "name": "layers",
      "desc": "Active layers. Default is no layers.",
      "optional": true,
      "type": "string"
    },
    {
      "name": "passThrough",
      "desc": "If set, loading screen will have no text and music will keep playing",
      "optional": true,
      "type": "boolean"
    }
  ],
  "returns": [],
  "examples": [
    [
      "function init()",
      "\t--Start level with no active layers",
      "\tStartLevel(\"level1\", \"MOD/level1.xml\")",
      "",
      "\t--Start level with two layers",
      "\tStartLevel(\"level1\", \"MOD/level1.xml\", \"vehicles targets\")",
      "end"
    ]
  ],
  "description": [
    "Start a level"
  ]
}
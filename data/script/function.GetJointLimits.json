{
  "name": "GetJointLimits",
  "arguments": [
    {
      "name": "joint",
      "desc": "Joint handle",
      "optional": false,
      "type": "number"
    }
  ],
  "returns": [
    {
      "name": "min",
      "desc": "Minimum joint limit (angle or distance)",
      "optional": false,
      "type": "number"
    },
    {
      "name": "max",
      "desc": "Maximum joint limit (angle or distance)",
      "optional": false,
      "type": "number"
    }
  ],
  "examples": [
    [
      "local min, max = GetJointLimits(hinge)"
    ]
  ],
  "description": [
    "Return joint limits for hinge or prismatic joint. Returns angle or distance",
    "depending on joint type."
  ]
}
{
  "name": "GetBodyShapes",
  "arguments": [
    {
      "name": "handle",
      "desc": "Body handle",
      "optional": false,
      "type": "number"
    }
  ],
  "returns": [
    {
      "name": "list",
      "desc": "Indexed table of shape handles",
      "optional": false,
      "type": "table"
    }
  ],
  "examples": [
    [
      "function init()",
      "\thandle = FindBody(\"body\", true)",
      "",
      "\tlocal shapes = GetBodyShapes(handle)",
      "\tfor i=1,#shapes do",
      "\t\tlocal shape = shapes[i]",
      "\t\tDebugPrint(shape)",
      "\tend",
      "end"
    ]
  ],
  "description": [
    "Return handles to all shapes owned by a body"
  ]
}
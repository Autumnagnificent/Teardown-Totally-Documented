{
  "name": "DrawShapeHighlight",
  "arguments": [
    {
      "name": "handle",
      "desc": "Shape handle",
      "optional": false,
      "type": "number"
    },
    {
      "name": "amount",
      "desc": "Amount",
      "optional": false,
      "type": "number"
    }
  ],
  "returns": [],
  "examples": [
    [
      "local shape = 0",
      "function init()",
      "\tshape = FindShape(\"shape\", true)",
      "end",

{
  "name": "GetBodyVelocity",
  "arguments": [
    {
      "name": "handle",
      "desc": "Body handle (should be a dynamic body)",
      "optional": false,
      "type": "number"
    }
  ],
  "returns": [
    {
      "name": "velocity",
      "desc": "Linear velocity as vector",
      "optional": false,
      "type": "TVec"
    }
  ],
  "examples": [
    [
      "function init()",
      "\thandle = FindBody(\"body\", true)",
      "\tlocal vel = Vec(0,10,0)",
      "\tSetBodyVelocity(handle, vel)",
      "end",
      "",
      "function tick()",
      "\tDebugPrint(VecStr(GetBodyVelocity(handle)))",
      "end"
    ]
  ],
  "description": [
    ""
  ]
}
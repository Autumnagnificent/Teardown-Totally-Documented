{
  "name": "TranslateBrush",
  "arguments": [
    {
      "name": "brush",
      "desc": "Brush handle",
      "optional": false,
      "type": "number"
    },
    {
      "name": "x",
      "desc": "Offset along x axis",
      "optional": false,
      "type": "number"
    },
    {
      "name": "y",
      "desc": "Offset along y axis",
      "optional": false,
      "type": "number"
    },
    {
      "name": "z",
      "desc": "Offset along z axis",
      "optional": false,
      "type": "number"
    }
  ],
  "returns": [],
  "examples": [
    [
      "function init()",
      "\tlocal brush = CreateBrush(\"brush/white.vox\")",
      "\tlocal brick = CreateMaterial(\"masonry\", 0.6, 0.5, 0.3, 0, 0.1, 0, 0)",
      "\tMaterial(brick)",
      "\tVox(0,0,0)",
      "\tBox(0, 0, 0, 10, 10, 10)",
      "\tTranslateBrush(brush, 5, 0, 0)",
      "\tBox(0, 0, 0, 10, 10, 10)",
      "end"
    ]
  ],
  "description": [
    "Apply a translation on the brush"
  ]
}